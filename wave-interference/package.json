{
  "name": "wave-interference",
  "version": "0.0.0",
  "description": "A wave interference simulation",
  "main": "main.js",
  "scripts": {
    "prepublish": "bower install",
    "test": "./node_modules/.bin/browserify src/js/main.js -o bundle.js",
    "start": "./node_modules/.bin/beefy src/js/main.js:bundle.js  8084 --live -- "
  },
  "browserify": {
    "transform": [
      "node-lessify",
      "debowerify",
      "deamdify"
    ]
  },
  "browser": {
    "models": "src/js/models",
    "views": "src/js/views",
    "utils": "src/js/utils",
    "lib": "src/js/lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Connexions/simulations.git"
  },
  "keywords": [
    "simulation",
    "sim"
  ],
  "bugs": {
    "url": "https://github.com/Connexions/simulations/issues"
  },
  "homepage": "https://github.com/Connexions/simulations",
  "dependencies": {},
  "devDependencies": {
    "beefy": "^2.1.1",
    "bower": "^1.3.9",
    "bower-resolve": "^2.2.0",
    "browserify": "^5.12.0",
    "deamdify": "^0.1.1",
    "debowerify": "^0.8.2",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^0.11.4",
    "grunt-contrib-requirejs": "^0.4.4",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-gh-pages": "^0.9.1",
    "grunt-mocha": "^0.4.11",
    "grunt-targethtml": "^0.2.6",
    "html2js-browserify": "0.0.2",
    "less": "^1.7.4",
    "matchdep": "^0.3.0",
    "node-lessify": "0.0.4-a",
    "watchify": "^1.0.6"
  }
}
